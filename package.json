{
  "name": "mongo-express-react",
  "version": "1.0.0",
  "description": "A full stack application with React and MongoDB (Easily deployed)",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "production": "PORT=8888 NODE_ENV=production npm run server-dev",
    "deploy": "npm run build && git commit -a -m deployment && git push heroku master",
    "dev": "webpack-dev-server --open",
    "server-dev": "nodemon src/server/server.js --exec babel-node src/server",
    "start": "npm run server",
    "server": "babel-node src/server/server",
    "populateDatabase": "babel-node src/server/populateDatabase",
    "test": "concurrently \"npm run server-dev\" \"npm run dev\""
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "mongo"
  ],
  "author": "Daniel 'The Code Whisperer' Stern",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "cors": "^2.8.4",
    "csv-parser": "^2.3.3",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "jsonwebtoken": "^  8.5.1",
    "material-table": "^1.61.0",
    "md5": "^2.2.1",
    "mongoose": "^5.9.9",
    "neat-csv": "^5.2.0",
    "react": "^16.4.2",
    "react-dom": "^16.5.0",
    "react-hook-form": "^4.7.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "uuid": "^3.3.2",
    "webpack": "^4.17.2"
  },
  "engines": {
    "node": "9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.2",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "highground": "^1.0.3-beta",
    "jest": "^23.6.0",
    "mongodb": "^3.4.1",
    "nodemon": "^1.18.4",
    "prettier": "2.0.5",
    "style-loader": "^0.23.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.7"
  }
}
